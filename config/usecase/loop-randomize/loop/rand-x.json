{
	sDTI: "/catharsys/manifest/control/loop/range:1.0",
	sId: "${filebasename}",

	__locals__: {
		iStart: "${action:args:iConfigStartX}",
		iCnt: "${action:args:iConfigCountX}"
	},

	iMin: "${iStart}",
	iMax: "$sum{${iStart}, ${iCnt}, -1}",
	iStep: 1,

	mIterationConfig: {
        sId: "$L{%0}",

		__globals__: {
			iSeed: "$int{$L{%0}}",
			// doSeed: "$rand.seed{$iSeed}",
			fX: "$rand.uniform{-1, 1}",
		}
	}
}
