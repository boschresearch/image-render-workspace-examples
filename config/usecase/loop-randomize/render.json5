{
    sDTI: "/catharsys/blender/render/output-list:1",
	sId: "${filebasename}",

	lSettings: [
        {
            // Settings for the render engine.
            // This is the same as the settings in the render panel.
            // For example, the element 'engine' refers to 'bpy.context.scene.render.engine'.
	        sDTI: "/catharsys/blender/render/settings/render:1",
            // Set the render engine to CYCLES.
            engine: "CYCLES",
        }
    ],

    lOutputs: [
        {
            sDTI: "/catharsys/blender/render/output/image:1",
            sId: "${filebasename}",

            mCompositor: {
                sDTI: "/catharsys/blender/compositor:1",
                sId: "${filebasename}",

                lFileOut: [
                    { 
                        sOutput: "Image",
                        sFolder: "Image",
                        sContentType: "image",
                        "mFormat" : { 
                            sFileFormat: "PNG",
                            sColorDepth: "8"
                        }
                    },
                ]
            }

        },
        {
            sDTI: "/catharsys/blender/render/output/anytruth/pos3d:1",
            sId: "${filebasename}",
        },

    ] // lOutputs
}

