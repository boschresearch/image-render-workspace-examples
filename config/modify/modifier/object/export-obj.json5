{
    sDTI: "/catharsys/blender/modify:1.0",

    sId: "${folder}/${filebasename}",
    
    mObjects: {
        // The object to export in OBJ file format
        Cube: [
            {
                // This modifier exports the selected object in OBJ file format
                sDTI: "/catharsys/blender/modify/object/export/obj:1.0",

                // The file path under which to store the OBJ file and its' textures.
                // Note that only textures that reference an external texture file
                // will be referenced in the `.mtl` that lies next to the `.obj` file.
                sFilePath: "[some filepath]",

                // Flag whether to create the target path
                bCreatePath: true,
            }
        ]    
    }
}


