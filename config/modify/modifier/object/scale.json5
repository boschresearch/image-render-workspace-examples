{
    sDTI: "/catharsys/blender/modify:1.0",

    sId: "${folder}/${filebasename}",
    
    mObjects: {
        Cube: [
            {
                // This modifier scales an object to the current scene units
                sDTI: "blender/modify/object/scale:1.0",

                // This defines the scaling of the object.
                lScale: [1.0, 1.0, 1.0],
            }
        ]    
    }
}


